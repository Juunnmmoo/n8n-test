      "id": "1295cd3d-cbcb-415f-804c-637078167319",
      "name": "Get brandpage"
    },
    {
      "parameters": {
        "jsCode": "// 모든 input 아이템 가져오기\nconst inputItems = $input.all();\n\nconst results = [];\n\nfor (const current of inputItems) {\n\n  const item = current.json;\n\n  // HTML 문자열 추출\n  const html =\n    typeof item === 'string' ? item :\n    item.body ? item.body :\n    item.data ? item.data :\n    null;\n\n  if (!html || typeof html !== 'string') {\n    results.push({\n      json: { error: 'HTML 문자열을 찾을 수 없음', received: item }\n    });\n    continue;\n  }\n\n  // STEP 1: \"브랜드 Best\" 위치 찾기\n  const marker = \"브랜드 Best\";\n  const index = html.indexOf(marker);\n\n  if (index === -1) {\n    results.push({\n      json: { error: '\"브랜드 Best\" 문구를 찾을 수 없음' }\n    });\n    continue;\n  }\n\n  // \"브랜드 Best\" 이후만 자르기\n  const slicedHtml = html.slice(index);\n\n  // STEP 2: 첫 상품 링크 추출\n  const match = slicedHtml.match(/\\/goods\\/detail\\/(\\d+)/);\n\n  if (!match) {\n    results.push({\n      json: { error: '브랜드 Best 이후에서 상품 링크를 찾지 못함' }\n    });\n    continue;\n  }\n\n  const goodsId = match[1];\n\n  // 정상 결과 추가\n  results.push({\n    json: {\n      goodsId,\n      link: `https://www.fashionplus.co.kr/goods/detail/${goodsId}`\n    }\n  });\n}\n\n// 모든 아이템 반환\nreturn results;\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1680,
