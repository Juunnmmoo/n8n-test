    {
      "parameters": {
        "jsCode": "// 1) brands 배열 가져오기\nlet brandsRaw = $('Check item count').all();\nlet brands;\n\n// brandsRaw[0].json 이 배열인지 아닌지 체크\nif (Array.isArray(brandsRaw[0].json)) {\n  brands = brandsRaw[0].json;\n} else {\n  brands = brandsRaw.map(i => i.json);\n}\n\n// 2) businessList 배열 가져오기\nlet businessRaw = $('Check not empty brand').all();\nlet businessList;\n\n// businessRaw[0].json 이 배열인지 아닌지 체크\nif (Array.isArray(businessRaw[0].json)) {\n  businessList = businessRaw[0].json;\n} else {\n  businessList = businessRaw.map(i => i.json);\n}\n\n// BrandName 정규화\nconst businessBrandNames = new Set(\n  businessList.map(b => (b.BrandName || \"\").replace(/,$/, \"\").trim())\n);\n\n// 브랜드 중복 제거\nconst filtered = brands.filter(b => !businessBrandNames.has(b.Name));\n\n// 반환\nreturn filtered.map(b => ({ json: b }));\n"
      },
      "type": "n8n-nodes-base.code",
