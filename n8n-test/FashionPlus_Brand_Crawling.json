      "id": "09bcd73c-4c0b-4c7b-b1c4-3c3a073149cb",
      "name": "Get product page"
    },
    {
      "parameters": {
        "jsCode": "const results = [];\n\nconst inputItems = $input.all();\n\nfor (const current of inputItems) {\n  // HTML 문자열 확보 (body → data → json 순서로 체크)\n  const html = String(\n    current.json.data ||\n    current.json.body ||\n    current.json ||\n    \"\"\n  );\n\n  // 1) \"판매자 정보\" 이후 첫 번째 <table> 블록 찾기\n  const sellerBlockMatch = html.match(/판매자 정보[\\s\\S]*?<table>([\\s\\S]*?)<\\/table>/);\n\n  if (!sellerBlockMatch) {\n    results.push({\n      json: {\n        error: \"판매자 정보 테이블을 찾을 수 없음\",\n        raw: current.json\n      }\n    });\n    continue;\n  }\n\n  const sellerTable = sellerBlockMatch[1];\n\n  // 2) 업체명 추출\n  const companyMatch = sellerTable.match(/업체명[\\s\\S]*?<td>(.*?)<\\/td>/);\n  const businessname = companyMatch ? companyMatch[1].trim() : null;\n\n  // 3) 연락처 추출\n  const phoneMatch = sellerTable.match(/연락처[\\s\\S]*?<td>(.*?)<\\/td>/);\n  const phone = phoneMatch ? phoneMatch[1].trim() : null;\n\n  // 아이템 결과 push\n  results.push({\n    json: {\n      businessname,\n      phone  \n    }\n  });\n}\n\nreturn results;\n"
      },
